<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="spaceAnchor" prefHeight="503.0" prefWidth="760.0" stylesheets="@MainStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SpaceshipGameController">
   <children>
      <ImageView fx:id="backgroundImg" fitHeight="503.0" fitWidth="798.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/planetbackground.png" />
         </image>
      </ImageView>
      <ImageView fx:id="spaceship" fitHeight="150.0" fitWidth="200.0" layoutX="305.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true" rotate="180.0">
         <image>
            <Image url="@images/spaceship.png" />
         </image>
      </ImageView>
      <ImageView fx:id="explosion" fitHeight="254.0" fitWidth="481.0" layoutX="252.0" layoutY="82.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/explosion.png" />
         </image>
      </ImageView>
      <Button fx:id="startBttn" layoutX="329.0" layoutY="318.0" mnemonicParsing="false" onAction="#handleStartBttn" prefHeight="37.0" prefWidth="102.0" text="Start" />
   </children>
</AnchorPane>
